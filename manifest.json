{
  "description": "Ativar modo escuro no Messenger",
  "manifest_version": 2,
  "name": "Messenger Dark",
  "author": "Johnendz",
  "version": "1.3",
  "homepage_url": "https://github.com/johnendz",

  "content_scripts": [
    {
	  "matches": ["*://*.messenger.com/*"],
      "css":["style.css"],
      "js": ["script.js"]
    }
  ],
  
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "96": "icons/96.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  }
}