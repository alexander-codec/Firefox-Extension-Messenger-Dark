{
  "description": "Ativar tema escuro no Messenger | Enable dark theme in Messenger",
  "manifest_version": 2,
  "name": "Messenger Dark (Tema Escuro)",
  "author": "Johnendz",
  "version": "2.1",

  "content_scripts": [
    {
	  "matches": ["*://*.messenger.com/*"],
      "css":["style.css"],
      "run_at": "document_start"
    }
  ],
  
  "icons": {
	"32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  }
}